{
  "name": "mongoose-fuzzy-search",
  "version": "0.0.1",
  "description": "fuzzy search based on trigrams for mongoose odm",
  "main": "./index.cjs",
  "module": "./index.js",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "mongoose",
    "mongodb",
